# Example .env file for social-media-susi
# Copy this file to .env and fill in your real secrets

# OneDrive
ONEDRIVE_APPLICATION_ID=your_application_id
ONEDRIVE_DIRECTORY_ID=your_onedrive_tenant_id
ONEDRIVE_CLIENT_SECRET=your_onedrive_client_secret
ONEDRIVE_CLIENT_SECRET_ID=your_onedrive_client_secret_id
ONEDRIVE_IMAGES_FOLDER_PATH_NEW=your_folder_path
ONEDRIVE_IMAGES_FOLDER_PATH_PROCESSED=your_folder_path
ONEDRIVE_POSTS_EXCEL_PATH=your_folder_path
ONEDRIVE_POSTS_EXCEL_SHEET_NAME=your_sheet_name
ONEDRIVE_GRAPH_ROOT=your_oncedrive_graph_root

# Instagram
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token
INSTAGRAM_USER_ID=your_instagram_user_id
INSTAGRAM_APP_ID=your_app_id
INSTAGRAM_APP_SECRET=your_app_secret

# AWS S3
AWS_S3_BUCKET_NAME=your_s3_bucket_name
AWS_S3_BUCKET_REGION=eu-central-1
AWS_S3_BUCKET_PROFILE=susi
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# NewsAPI
NEWSAPI_KEY=your_news_api_key

# Gmail API (OAuth2)
GMAIL_SENDER=your_gmail_address@gmail.com
GMAIL_RECIPIENT=your_gmail_address@gmail.com
GMAIL_SECRET_FILE=client_secret.json
GMAIL_TOKEN_FILE=gmail_token.json

# GenAI API
LOCAL_GENAI_API_URL=your_local_api_url

# (Optional) Any other secrets or config variables
